   ],
   "source": [
    "def common_elements(list1,list2):\n",
    "    list3=[]\n",
    "    for i in list1:\n",
    "        for j in list2:\n",
    "            if i==j:\n",
    "                list3.append(i)\n",
    "    return list3\n",
    "\n",
    "common_elements([1,2,3,4],[1,2,8,9])"
   ]
